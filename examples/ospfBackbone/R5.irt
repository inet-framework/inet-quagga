ifconfig:

# ethernet card 0 of router - connected to N5
name: eth0
  inet_addr: 192.168.5.3
  Mask: 255.255.255.0
  Groups: 224.0.0.5:224.0.0.6
  MTU: 1500
  Metric: 5
  BROADCAST MULTICAST

# ethernet card 1 of router - connected to N4
name: eth1
  inet_addr: 192.168.4.2
  Mask: 255.255.255.0
  Groups: 224.0.0.5:224.0.0.6
  MTU: 1500
  Metric: 4
  BROADCAST MULTICAST

ifconfigend.

route:

224.0.0.0 * 240.0.0.0   H 0 eth0
224.0.0.0 * 240.0.0.0   H 0 eth1

routeend.
